{
  "name": "lds-pgp2021",
  "author": "Orie Steele",
  "module": "dist/lds-pgp2021.esm.js",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "pgp2021": "./bin/cli.js"
  },
  "scripts": {
    "pgp2021": "node ./bin/cli.js",
    "start": "tsdx watch",
    "build": "tsdx build",
    "pretest": "export TS_JEST_DISABLE_VER_CHECKER=true",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "npm publish"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/lds-pgp2021.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/lds-pgp2021.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@transmute/vc.js": "^0.2.1-unstable.0",
    "husky": "^4.3.8",
    "size-limit": "^4.9.1",
    "ts-jest": "^26.4.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@transmute/jsonld-document-loader": "^0.2.0",
    "@transmute/linked-data-proof": "^0.2.1-unstable.0",
    "jsonld": "^3.2.0",
    "jsonld-signatures": "^6.0.0",
    "node-fetch": "^2.6.1",
    "openpgp": "^4.10.9",
    "security-context": "^4.0.0",
    "vorpal": "^1.12.0"
  }
}
